<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cazza223.mhealthapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mhealth_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".user_area" />
        <activity
            android:name=".AtRestDataActivity"
            android:label="@string/title_activity_at_rest_data"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActiveDataActivity"
            android:label="@string/title_activity_active_data"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".drug_survey"></activity>
    </application>

</manifest>